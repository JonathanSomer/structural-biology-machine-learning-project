from utils.pdb_parser_wrapper import PDBParserWrapper
from utils.pdb_utils import get_structure_sequence

receptor = "5cha"
ligand = "2ovo"

parser = PDBParserWrapper()
receptor_struct = parser.get_receptor_structure("x", receptor, ligand)
ligand_struct = parser.get_ligand_structure("y", receptor, ligand)

print(get_structure_sequence(receptor_struct))
print(get_structure_sequence(ligand_struct))